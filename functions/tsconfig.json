{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/server*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "module": "CommonJS",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017"
  },
  "compileOnSave": true,
  "include": ["src"],
  "baseUrl": "."
}
